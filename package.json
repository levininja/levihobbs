{
  "name": "levihobbs_claude_sonnet",
  "version": "1.0.0",
  "description": "This is a personal website built using ASP.NET Core MVC with PostgreSQL as the database.",
  "main": "index.js",
  "scripts": {
    "scss": "node-sass --output-style compressed --source-map true wwwroot/css/scss -o wwwroot/css",
    "scss:watch": "nodemon --watch wwwroot/css --ext scss --exec \"npm run scss\"",
    "start": "npm-run-all --parallel scss:watch",
    "build": "npm run scss",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "node-sass": "^9.0.0",
    "nodemon": "^3.1.10",
    "npm-run-all": "^4.1.5"
  }
}
